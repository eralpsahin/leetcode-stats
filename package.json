{
  "name": "leetcode-stats",
  "productName": "Stats for LeetCode",
  "version": "0.1.0",
  "description": "Electron app for keeping track of your LeetCode progress.",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/alexdevero/electron-react-webpack-boilerplate.git"
  },
  "homepage": "https://eralpsahin.com",
  "bugs": {
    "url": "https://github.com/eralpsahin/leetcode-stats/issues"
  },
  "author": {
    "name": "Eralp Sahin"
  },
  "keywords": [
    "leetcode",
    "stats",
    "api",
    "electron",
    "programming",
    "coding",
    "grinding",
    "interview",
    "career"
  ],
  "engines": {
    "node": ">=9.0.0",
    "npm": ">=5.0.0",
    "yarn": ">=1.0.0"
  },
  "main": "main.js",
  "scripts": {
    "prod": "webpack --mode production --config webpack.build.config.js && electron --noDevServer .",
    "start": "webpack-dev-server --hot --host 0.0.0.0 --config=./webpack.dev.config.js --mode development",
    "build": "webpack --config webpack.build.config.js --mode production",
    "package": "npm run build",
    "postpackage": "electron-packager ./ --out=./builds --overwrite --icon='./src/assets/icon/icon.icns'",
    "lint": "eslint src/**/*.js --ext src/**/*.jsx",
    "format": "prettier --write \"src/**/*.{js,jsx}\""
  },
  "dependencies": {
    "@material-ui/core": "^4.1.3",
    "@material-ui/icons": "^4.2.1",
    "axios": "^0.19.0",
    "cheerio": "^1.0.0-rc.3",
    "electron": "^7.2.4",
    "electron-is-dev": "^1.1.0",
    "material-table": "^1.40.1",
    "material-ui": "^0.20.2",
    "react": "^16.8.6",
    "react-calendar-heatmap": "^1.8.0",
    "react-dom": "^16.8.6",
    "react-minimal-pie-chart": "^4.2.0",
    "react-router-dom": "^5.0.1",
    "react-tooltip": "^3.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babili-webpack-plugin": "^0.1.2",
    "css-loader": "^2.1.1",
    "electron-packager": "^13.1.1",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "eslint-plugin-standard": "^4.0.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.7.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.1.2",
    "postcss-pxtorem": "^4.0.1",
    "prettier": "^1.18.2",
    "style-loader": "^0.23.1",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.5.1"
  }
}
